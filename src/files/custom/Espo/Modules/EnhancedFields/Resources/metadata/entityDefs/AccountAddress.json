{
	"collection": {
		"order": "asc",
		"orderBy": "name"
	},
	"fields": {
		"account": {
			"required": true,
			"type": "link"
		},
		"city": {
			"maxLength": 100,
			"type": "varchar"
		},
		"country": {
			"maxLength": 100,
			"type": "varchar"
		},
		"createdAt": {
			"readOnly": true,
			"type": "datetime"
		},
		"description": {
			"type": "wysiwyg"
		},
		"invalid": {
			"type": "bool"
		},
		"modifiedAt": {
			"readOnly": true,
			"type": "datetime"
		},
		"name": {
			"index": true,
			"maxLength": 512,
			"readOnly": true,
			"required": true,
			"type": "varchar",
			"view": "views/fields/varchar"
		},
		"postalCode": {
			"maxLength": 40,
			"type": "varchar"
		},
		"primary": {
			"notStorable": true,
			"type": "bool"
		},
		"state": {
			"maxLength": 100,
			"type": "varchar"
		},
		"street": {
			"dbType": "varchar",
			"maxLength": 255,
			"type": "text"
		},
		"type": {
			"allowCustomOptions": true,
			"displayAsLabel": true,
			"maxCount": 1,
			"options": [
				"",
				"Head Office",
				"Satellite Office",
				"Regional Office",
				"Factory",
				"Distribution Center",
				"Construction Site",
				"Billing Address"
			],
			"type": "multiEnum"
		}
	},
	"indexes": {
		"name": {
			"columns": [
				"name"
			]
		}
	},
	"links": {
		"account": {
			"entity": "Account",
			"foreign": "accountAddresses",
			"relationName": "entityAccountAddress",
			"type": "belongsTo"
		}
	}
}